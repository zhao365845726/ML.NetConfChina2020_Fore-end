page {
  background: #f6f6f6;
}

.car-head {
  width: 100%;
  height: 108rpx;
  background: linear-gradient(226deg, #5ade95 0%, #36c665 100%);
  position: relative;
  overflow: hidden;
}

.car-head-in {
  line-height: 48rpx;
  height: 48rpx;
  color: #fff;
  margin-top: 24rpx;
  position: relative;
}

.car-title {
  font-size:34rpx;
  text-align: center;
  font-weight: bold;
}

.car-edit {
  font-size: 30rpx;
  position: absolute;
  right: 32rpx;
  top: 0;
}

.car-border {
  position: absolute;
  background: #fff;
  width: 100%;
  border-radius: 16rpx 16rpx 0px 0px;
  bottom: 0;
  left: 0;
  height: 20rpx;
  box-sizing: border-box;
}

.car-list {
  height: 236rpx;
  background: #fff;
  box-sizing: border-box;
  position: relative;
  padding: 0 32rpx;
}

.car-choice {
  display: inline-block;
  width: 36rpx;
  height: 36rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  margin-top: 100rpx;
}

.car-product {
  width: 610rpx;
  display: inline-block;
  height: 236rpx;
  border-bottom: 0.5px solid #eee;
  margin-left: 30rpx;
  box-sizing: border-box;
  padding-top: 32rpx;
  vertical-align: top;
}

.car-cover {
  width: 172rpx;
  height: 172rpx;
  border-radius: 16rpx;
  object-fit: cover;
}

.car-detail {
  width: 414rpx;
  margin-left: 16rpx;
  vertical-align: top;
  display: inline-block;
}

.car-name {
  color: #333;
  font-size: 28rpx;
  line-height: 40rpx;
  max-width: 400rpx;
  overflow: hidden;
  font-weight: bold;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.car-origin {
  color: #999;
  font-size: 26rpx;
  margin-top: 32rpx;
  height: 20rpx;
}

.car-bto {
  height: 48rpx;
  margin-top: 16rpx;
}

.car-price {
  display: inline-block;
  color: #ff7622;
  font-size: 24rpx;
  line-height:34rpx;
}

.car-price span {
  font-weight: bold;
  font-size: 32rpx;
  line-height: 48rpx;
}

.car-num {
  float: right;
  width: 132rpx;
  height: 48rpx;
  position: relative;
}

.car-reduce {
  width: 22rpx;
  height: 22rpx;
  background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABYAAAAWCAYAAADEtGw7AAAAa0lEQVRIS2NkoBFgpJG5DKMGw0N2NCjoFBROTk7KzMzMG6mRpv/+/eu/b9++u+DIc3Z21mJkZLxKDYP///+vvXfv3mu0NdjS0pKTm5tbnxou/vr168Xjx49/H03HdErH1Ig0dDNGI4/2kQcAo9QgF7kvfH4AAAAASUVORK5CYII=);
  background-size: 100% 100%;
  left: 0;
}

.car-number {
  width: 64rpx;
  height: 42rpx;
  background: #f6f6f6;
  left: 32rpx;
  line-height: 42rpx;
  color: #323233;
  font-size: 24rpx;
  font-weight: bold;
  text-align: center;
}

.car-add {
  width: 22rpx;
  height: 22rpx;
  background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABYAAAAWCAYAAADEtGw7AAAAmklEQVRIS2NkoBFgJMZcNzc3sb9//4qA1DIzM7/ZtWvXK0L6iDLY1dW17f///5UgwxgZGdt3795dNWrwaFCQmSpA6fT///8FuJLQv3//7BkZGa1A8v///z/GxMR0EJdaRkbGCaB0Dk7Hzs7OWoyMjFcJpU1i5P///6+9d+/ea7Q1mGZBQciLo1kaHkKjQUH7oKBZ1UQonWOTBwANPLAX0SrLqwAAAABJRU5ErkJggg==);
  background-size: 100% 100%;
  right: 0;
}

.fix-full {
  height: 162rpx;
}

.fix-bto {
  width: 100%;
  height: 98rpx;
  left: 0;
  bottom: 0;
  position: fixed;
  background: #fff;
}

.all-choice {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  left: 32rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}

.all-choice-text {
  line-height: 98rpx;
  color: #333;
  font-size: 30rpx;
  left: 84rpx;
}

.all-price {
  color: #ff7622;
  font-size: 24rpx;
  position: absolute;
  right: 248rpx;
  line-height:94rpx;
  top: 0rpx;
  height: 98rpx;
}

.all-price span {
  font-weight: bold;
  line-height: 48rpx;
  font-size: 32rpx;
}

.vip-price {
  color: #999;
  line-height: 32rpx;
  font-size: 22rpx;
  position: absolute;
  right: 248rpx;
  bottom: 12rpx;
}

.confirm {
  width: 200rpx;
  height: 70rpx;
  background: #36c665;
  border-radius: 40rpx;
  line-height: 70rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  font-weight: bold;
  position: absolute;
  right: 32rpx;
}

.fix-subvip {
  width: 100%;
  height: 64rpx;
  position: fixed;
  bottom: 98rpx;
  left: 0;
  background: #fff6e7;
  font-size: 24rpx;
  color: #333;
  line-height: 64rpx;
  text-align: center;
}

.green {
  color: #36c665;
}

.check-on {
  background-image: url(data:image/png;base64,);
}

.reduce-g {
  background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABYAAAAWCAYAAADEtGw7AAAAa0lEQVRIS2NkoBFgpJG5DKMGw0N2NCjoFBQfPnxQ/vfv30ZqpGkmJiZ/AQGBu+DIe/v2rRYjI+NVahj8//9/bWFh4Wu0Nfjx48ecfHx8+tRw8adPny7Kysp+H03HdErH1Ig0dDNGI4/2kQcA+5EgF/gDXmYAAAAASUVORK5CYII=);
}

.all-choice-n {
  background-image: url(data:image/png;base64,);
}

.topone {
  height: 216rpx;
}

.car-product-one {
  padding-top: 12rpx;
  height: 216rpx;
}

.car-choice-one {
  margin-top: 80rpx;
}

.no-data {
  background: #f6f6f6;
}

.no-data-empty {
  background: #f6f6f6;
}

.no-border {
  background: #f6f6f6;
}
